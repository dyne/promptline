{
  "api_key": "your-dashscope-api-key",
  "api_url": "https://dashscope-intl.aliyuncs.com/compatible-mode/v1",
  "model": "qwen3",
  "temperature": 0.7,
  "max_tokens": 1500,
  "history_file": ".promptline_conversation_history",
  "command_history_file": ".promptline_history",
  "history_max_messages": 100,
  "tools": {
    "allow": ["get_current_datetime", "read_file", "ls"],
    "ask": [
      "write_file",
      "cat",
      "cp",
      "mv",
      "rm",
      "ln",
      "touch",
      "truncate",
      "readlink",
      "realpath",
      "mkdir",
      "pwd",
      "dirname",
      "basename",
      "grep",
      "head",
      "tail",
      "sort",
      "uniq",
      "wc",
      "tr",
      "tee",
      "comm",
      "strings",
      "more",
      "hexdump",
      "cmp",
      "md5sum",
      "shasum",
      "base64",
      "uname",
      "hostname",
      "uptime",
      "free",
      "df",
      "du",
      "ps",
      "pidof",
      "id",
      "echo",
      "seq",
      "printenv",
      "tty",
      "which",
      "mkfifo",
      "mktemp",
      "find",
      "chmod",
      "date"
    ]
  },
  "tool_limits": {
    "max_file_size_bytes": 10485760,
    "max_directory_depth": 8,
    "max_directory_entries": 2000
  },
  "tool_rate_limits": {
    "default_per_minute": 60,
    "per_tool": {},
    "cooldown_seconds": {}
  },
  "tool_timeouts": {
    "default_seconds": 0,
    "per_tool_seconds": {}
  }
}
